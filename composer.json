{
    "name": "peroxovy/laravel-mail-logging",
    "description": "Package which allows to send logs to e-mail.",
    "type": "library",
	"require": {
        "php": ">=7.4",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "monolog/monolog": "^2.0 || ^3.0"
    },
    "license": "MIT",
	"autoload": {
        "psr-4": {
            "Peroxovy\\LaravelMailLogging\\": "src/"
        }
    },
    "authors": [
        {
            "name": "peroxovy",
			"email": "piotr.mazur.mail@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Peroxovy\\LaravelMailLogging\\TeamsLoggingServiceProvider"
            ],
            "aliases": {
                "MailLogging": "Peroxovy\\LaravelMailLogging\\MailLogging"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
